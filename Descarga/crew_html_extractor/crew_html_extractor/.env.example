# LLM por defecto (OpenAI). Cambia según tu proveedor o usa Ollama/LiteLLM.
OPENAI_API_KEY=sk-...

# Modelo por defecto (CrewAI suele usar gpt-4o-mini si no se especifica)
OPENAI_MODEL_NAME=gpt-4o-mini

# Parámetros del extractor (opcionales)
CHE_HTTP_TIMEOUT_S=30
CHE_VERIFY_TLS=true
CHE_RENDERED_WAIT_MS=1500
CHE_OUTPUT_DIR=outputs
